#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int arr[3][3];
    // 9 символов можно считать и без циклов, но с ними красивее
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            scanf("%d", &arr[i][j]);
        }
    }
    
    printf("\nTranspose of the matrix:\n");
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            printf("%d ", arr[j][i]);
        }
        printf("\n");
    }
}
